
public class Hello9 {

  
  public static void main(String[] args)
  { 
    int[] numbers = {40,27,82,14,19,65};
    
    int total = addNumbers(numbers);
    System.out.print(total);
  }
  
  public static int addNumbers(int[] nums)
  {
    int sum = 0;
//    for(int i = 0; i < nums.length; i++)
//    {
//      sum += nums[i];
//    }
    
    
    int i = 0;
    while(i < nums.length)
    {
      sum += nums[i];
      i++;
    }
    
    return sum;
  }
  
  
}
